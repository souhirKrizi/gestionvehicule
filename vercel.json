{
  "version": 2,
  "builds": [
    {
      "src": "public/index.php",
      "use": "@vercel/php"
    },
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "routes/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://gestionvehicule.vercel.app",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "SESSION_SECURE_COOKIE": "true",
    "VIEW_COMPILED_PATH": "/tmp/storage/framework/views"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}
